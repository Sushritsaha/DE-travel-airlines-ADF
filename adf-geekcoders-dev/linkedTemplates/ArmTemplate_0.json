		},
		"LS_RestAPI_airline_source_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://airlabs.co/api/v9/airlines"
		},
		"ls_AzureKeyVault_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://kv-geekcoders-dev.vault.azure.net/"
		},
		"ls_ADLS_sink_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://geekcodersdldev.dfs.core.windows.net/"
		},
		"ls_ADLS_sink_properties_typeProperties_tenant": {
