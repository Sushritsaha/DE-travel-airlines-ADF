						"typeProperties": {
							"method": "GET",
							"headers": {},
							"url": "https://kv-geekcoders-dev.vault.azure.net/secrets/airine-rest-api/53ad285ee66e4a41818f106f07a7e2b5?api-version=7.0",
							"authentication": {
								"type": "MSI",
								"resource": "https://vault.azure.net"
